    "cordova-plugin-ml-text": "^3.1.0",
    "cordova-plugin-network-information": "^3.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.0.0",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
